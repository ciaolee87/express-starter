{
  "name": "express-starter",
  "version": "0.0.1",
  "description": "Simple Express Server",
  "scripts": {
    "pm2": "NODE_ENV=development pm2 start ./build/app.js --name app.name",
    "pm2prod": "NODE_ENV=production pm2 start ./build/app.js --name app.name",
    "pm2test": "NODE_ENV=test pm2 start ./build/app.js --name app.name",
    "start": "nodemon --watch src --delay 1 --exec 'ts-node' --files src/App.ts ",
    "update": "ncu -u && npm i "
  },
  "test": {
    "test": "mocha -r ts-node/register test/**/test.ts"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
