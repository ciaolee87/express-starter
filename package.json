{
  "name": "ubi",
  "version": "1.0.0",
  "description": "Simple billing note database",
  "main": "app.js",
  "scripts": {
    "pm2": "tsc && pm2 start ./resources/js/pm2.config.js --env development",
    "pm2prod": "tsc && pm2 start ./resources/js/pm2.config.js --env production",
    "pm2test": "tsc && pm2 start ./resources/js/pm2.config.js --env test",
    "build": "tsc --build ./tsconfig.json",
    "start": "nodemon --watch src --delay 1 --exec 'ts-node' --files src/App.ts ",
    "webpack": "webpack --config ./resources/js/webpack.config.js --optimize-minimize",
    "webpackStart": "nodemon --watch src --delay 1 --exec 'node' dist/bundle.js"
  },
