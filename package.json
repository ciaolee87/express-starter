    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^3.4.6",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.2",
    "mocha-typescript": "^1.1.17",
    "nodemon": "^1.19.4",
    "npm-check-updates": "^3.2.0",
    "pm2": "^4.1.2",
    "source-map-support": "^0.5.16",
