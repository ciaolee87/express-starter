{
  "name": "express-starter",
  "version": "0.0.1",
  "description": "Simple Express Server",
  "scripts": {
    "build": "tsc --build ./tsconfig.json",
    "pm2": "NODE_ENV=development pm2 start ./build/app.js --name app.name",
    "pm2prod": "NODE_ENV=production pm2 start ./build/app.js --name app.name",
    "pm2test": "NODE_ENV=test pm2 start ./build/app.js --name app.name",
    "start": "nodemon --watch src --delay 1 --exec 'ts-node' --files src/App.ts ",
    "update": "ncu -u && yarn install"
  },
  "test": {
    "test": "mocha -r ts-node/register test/**/test.ts"
  },
  "dependencies": {

    "cookie-parser": "1.4.5",
    "debug": "4.1.1",
    "decimal.js": "10.2.0",
    "dotenv": "8.2.0",
    "es6-shim": "0.35.5",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "glob": "7.1.6",
    "helmet": "3.22.0",
    "hpp": "0.2.3",
    "jsonwebtoken": "8.5.1",
    "moment": "2.24.0",
    "multer": "1.4.2",
    "mysql2": "2.1.0",
    "node-schedule": "1.3.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.5",
    "sequelize": "5.21.12",
    "sequelize-typescript": "1.1.0",
    "uuid": "8.1.0",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "4.4.2"
  },
  "devDependencies": {
    "@types/validator": "13.0.0",
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@types/bluebird": "3.5.32",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.6",
    "@types/express-session": "1.17.0",
    "@types/helmet": "0.0.47",
    "@types/hpp": "0.2.1",
    "@types/jsonwebtoken": "8.5.0",
    "@types/mocha": "7.0.2",
    "@types/multer": "1.4.3",
    "@types/node": "14.0.11",
    "@types/node-schedule": "1.3.0",
    "@types/redis": "2.8.22",
    "@types/sequelize": "4.28.9",
    "@types/uuid": "8.0.0",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "mocha": "7.2.0",
    "mocha-typescript": "1.1.17",
    "nodemon": "2.0.4",
    "npm-check-updates": "6.0.1",
    "pm2": "4.4.0",
    "source-map-support": "0.5.19",
    "ts-loader": "7.0.5",
    "ts-node": "8.10.2",
    "typescript": "3.9.5"
  }
}
