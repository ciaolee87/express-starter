    "decimal.js": "^10.2.0",
    "dotenv": "^8.2.0",
    "es6-shim": "^0.35.5",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "firebase": "^7.2.3",
    "firebase-admin": "^8.7.0",
    "glob": "^7.1.5",
    "helmet": "^3.21.2",
    "hpp": "^0.2.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "multer": "^1.4.2",
    "mysql2": "^2.0.0",
    "node-schedule": "^1.3.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "sequelize": "^5.21.2",
    "sequelize-typescript": "^1.0.0",
    "uuid": "^3.3.3",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.2.1"
